{
  "name": "angled-edges-generator",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://angled-edges-generator.josephfus.co/",
  "dependencies": {
    "antd": "^3.11.6",
    "node-sass": "^4.11.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.6.2",
    "query-string": "^6.2.0",
    "react": "^16.6.3",
    "react-color": "^2.17.0",
    "react-dom": "^16.6.3",
    "react-draggable": "^3.1.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "react-syntax-highlighter": "^10.1.2",
    "styled-components": "^4.1.3",
    "tinycolor2": "^1.4.1",
    "utf8": "^3.0.0"
  },
  "scripts": {
    "start": "NODE_PATH=src react-scripts start",
    "build": "NODE_PATH=src react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "key-spacing": [
        1,
        {
          "singleLine": {
            "beforeColon": false,
            "afterColon": true
          },
          "multiLine": {
            "align": "colon",
            "mode": "minimum"
          }
        }
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 9",
    "not op_mini all"
  ],
  "devDependencies": {
    "gh-pages": "^2.0.1"
  }
}
